{
    "display_information": {
        "name": "Debugging Disciples",
        "description": "Workspace Bot",
        "background_color": "#4370f7"
    },
    "features": {
        "bot_user": {
            "display_name": "Debugging Disciples",
            "always_online": false
        }
    },
    "oauth_config": {
        "scopes": {
            "bot": [
                "channels:history",
                "channels:write.invites",
                "chat:write",
                "app_mentions:read",
                "im:history",
                "groups:history",
                "mpim:history"
            ]
        }
    },
    "settings": {
        "event_subscriptions": {
            "bot_events": [
                "channel_history_changed",
                "message.channels",
                "message.groups",
                "message.im"
            ]
        },
        "interactivity": {
            "is_enabled": true
        },
        "org_deploy_enabled": false,
        "socket_mode_enabled": true,
        "token_rotation_enabled": false
    }
}